<?xml version="1.0"?>

<launch>

  <!-- Run the map server 
  <arg name="map_file" default="$(find jackal_navigation)/maps/jackal_race.yaml"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />-->
  
  <!--- NEED TO OCCUPANCY GRID  -->

  <node pkg="pointcloud_to_laserscan" type="pointcloud_to_laserscan_node" name="pointcloud_to_laserscan_node" output="screen" >
  	<remap from="cloud_in" to="/front/points2"/>
  </node>

  <!--- Run AMCL -->
  <include file="$(find jackal_navigation)/launch/include/amcl.launch">
    <arg name="use_map_topic" value="true" />
    <arg name="scan_topic"    value="/scan" />
  </include>

  <!--- Run Move Base -->
 <include file="$(find jackal_navigation)/launch/include/move_base.launch" />

</launch>
